{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ttwa-web-client",
      "image": "dtrodger/ttwa-web-client",
      "hostname": "web_client",
      "essential": false
    },
    {
      "name": "ttwa-web-server",
      "image": "dtrodger/ttwa-web-server",
      "hostname": "web_server",
      "essential": false
    },
    {
      "name": "ttwa-worker",
      "image": "dtrodger/ttwa-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "ttwa-proxy",
      "image": "dtrodger/ttwa-proxy",
      "hostname": "proxy",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "web_server",
        "web_client"
      ]
    }
  ]
}